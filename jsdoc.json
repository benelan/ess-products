{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown", "better-docs/component"],
  "opts": {
    "destination": "docs",
    "readme": "./README.md",
    "recurse": true,
      "template": "./node_modules/better-docs"
  },
  "templates": {
    "better-docs": {
      "name": "Product2Queue Doc"
    }
  }
}